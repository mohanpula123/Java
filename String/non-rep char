
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        String str = "evloper";
        
        for (int i=0;i<str.length();i++) {
        	char c =str.charAt(i);
        	int count=0;
        	for (int j=0;j<str.length();j++) {
        		char k = str.charAt(j);
        		if (c==k) {
        			count++;
        		}
        	}
            if (count==1) {
            	System.out.println(c);
            	break;
            }
        }
	}

}
