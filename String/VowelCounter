
public class pro5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        String str = "mysore";
        String vowels = "AEIOUaeiou";
        int vowelcount=0;
        int constracount=0;
        for (int i=0;i<str.length();i++) {
        	char c = str.charAt(i);
           
        	if (vowels.contains(c+"")) {
        		vowelcount++;
        	}
        	else {
        		constracount++;
        	}
        }
        System.out.println("vowel count is >"+vowelcount);
	    
	}

}
